{
  "name": "rawr/cross-data-providers",
  "description": "Allows you to use mix multiple PhpUnit @dataProvider without duplication",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "TRegx\\": "src/TRegx"
    }
  },
  "autoload-dev": {
    "classmap": [
      "test/"
    ],
    "psr-4": {
      "Test\\": "test/"
    }
  },
  "require": {
    "php": "^7.0 || ^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "php-coveralls/php-coveralls": "^2.4"
  }
}
