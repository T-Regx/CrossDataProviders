{
  "name": "rawr/cross-data-providers",
  "description": "Allows you to use mix multiple PhpUnit @dataProvider without duplication",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "TRegx\\": "src/TRegx"
    }
  },
  "autoload-dev": {
    "classmap": [
      "test/"
    ],
    "psr-4": {
      "Test\\": "test/"
    }
  },
  "require": {
    "php": "=>5.6"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.7",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "archive": {
    "exclude": [
      "/test/**",
      "/test"
    ]
  }
}
