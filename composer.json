{
  "name": "rawr/cross-data-providers",
  "description": "Allows you to mix multiple PhpUnit @dataProvider without duplication",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "TRegx\\DataProvider\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "test/"
    ],
    "psr-4": {
      "Test\\TRegx\\DataProvider\\": "test/"
    }
  },
  "require": {
    "php": "^7.0 || ^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "php-coveralls/php-coveralls": "^2.4",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "scripts": {
    "ci": [
      "@ci:static"
    ],
    "ci:static": [
      "@phpcs",
      "@phpstan"
    ],
    "phpcbf": "phpcbf",
    "phpcs": "phpcs",
    "phpstan": "tools/phpstan.phar",
    "phpstan:baseline": "tools/phpstan.phar --generate-baseline"
  },
  "scripts-descriptions": {
    "phpcs": "Checks the coding style of the PHP files."
  }
}
